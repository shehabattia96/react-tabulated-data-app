{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "WebApp Mock",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "start"
            ],
            "env": {
                "REACT_APP_USE_MOCK": "true"
            },
            "cwd": "${workspaceFolder}/webapp",
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "WebApp",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "start"
            ],
            "cwd": "${workspaceFolder}/webapp",
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}